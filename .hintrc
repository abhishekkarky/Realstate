{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "meta-viewport": "off"
  }
}